{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "projects": {
    "logger": {
      "type": "library",
      "root": "libs/logger",
      "entryFile": "index",
      "sourceRoot": "libs/logger",
      "compilerOptions": {
        "tsConfigPath": "libs/logger/tsconfig.lib.json"
      }
    },
    "config": {
      "type": "library",
      "root": "config",
      "entryFile": "index",
      "sourceRoot": "config",
      "compilerOptions": {
        "tsConfigPath": "config/tsconfig.lib.json"
      }
    },
    "middleware": {
      "type": "library",
      "root": "middleware",
      "entryFile": "index",
      "sourceRoot": "middleware",
      "compilerOptions": {
        "tsConfigPath": "middleware/tsconfig.lib.json"
      }
    },
    "interceptor": {
      "type": "library",
      "root": "interceptor",
      "entryFile": "index",
      "sourceRoot": "interceptor",
      "compilerOptions": {
        "tsConfigPath": "interceptor/tsconfig.lib.json"
      }
    }
  },
  "compilerOptions": {
    "webpack": true
  }
}