{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true
  },
  "projects": {
    "middleware": {
      "type": "library",
      "root": "middleware",
      "entryFile": "main",
      "sourceRoot": "middleware",
      "compilerOptions": {
        "tsConfigPath": "middleware/tsconfig.lib.json"
      }
    },
    "interfaces": {
      "type": "library",
      "root": "interfaces",
      "entryFile": "main",
      "sourceRoot": "interfaces",
      "compilerOptions": {
        "tsConfigPath": "interfaces/tsconfig.lib.json"
      }
    },
    "config": {
      "type": "library",
      "root": "config",
      "entryFile": "main",
      "sourceRoot": "config",
      "compilerOptions": {
        "tsConfigPath": "config/tsconfig.lib.json"
      }
    },
    "libs": {
      "type": "library",
      "root": "libs",
      "entryFile": "main",
      "sourceRoot": "libs",
      "compilerOptions": {
        "tsConfigPath": "libs/tsconfig.lib.json"
      }
    },
    "mysql": {
      "type": "library",
      "root": "mysql",
      "entryFile": "main",
      "sourceRoot": "mysql",
      "compilerOptions": {
        "tsConfigPath": "mysql/tsconfig.lib.json"
      }
    },
    "utils": {
      "type": "library",
      "root": "utils",
      "entryFile": "main",
      "sourceRoot": "utils",
      "compilerOptions": {
        "tsConfigPath": "utils/tsconfig.lib.json"
      }
    }
  }
}
